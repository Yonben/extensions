{
  "name": "bitbucket",
  "title": "Bitbucket",
  "description": "Integration with Bitbucker",
  "icon": "command-icon.png",
  "author": "Yonben",
  "license": "MIT",
  "commands": [
    {
      "name": "search-repo",
      "title": "Search repo",
      "subtitle": "",
      "description": "Search a specific repo",
      "mode": "view",
      "preferences": [
        {
          "name": "username",
          "type": "textfield",
          "required": true,
          "title": "Username",
          "description": "Account Username",
          "link": "https://bitbucket.org/account/settings/"
        },
        {
          "name": "app_password",
          "type": "password",
          "required": true,
          "title": "App Password",
          "description": "An App Password generated in Bitbucket",
          "link": "https://bitbucket.org/account/settings/app-passwords/"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.25.2",
    "node-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}
